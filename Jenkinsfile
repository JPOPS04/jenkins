pipeline {
    agent any
    stages {
        stage('Pipeline Stages'){
            steps {
                sh "ls"
            }
        }
        stage('Stage2'){
            steps {
               sh "pwd"
            }
        }
        stage('Stage3'){
            steps {
               sh "mv"
            }
        }
        stage('Stage4'){
            steps {
               echo 'hello new'
            }
        }
        stage('Stage5'){
            steps {
               sh "touch"
            }
        }
    }
}
        
